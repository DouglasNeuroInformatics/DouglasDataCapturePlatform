{
  "name": "@dnp/common",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.ts",
  "types": "lib/index.ts",
  "devDependencies": {
    "@dnp/tsconfig": "*"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint lib/**/* --fix"
  }
}

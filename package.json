{
  "name": "@dnp/douglas-data-capture-platform",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "license": "AGPL-3.0",
  "scripts": {
    "build": "turbo run build",
    "clean": "fd -H -I --prune \".turbo|dist|node_modules\" --exec rm -rf",
    "dev": "turbo run dev",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@dnp/eslint-config": "*",
    "@dnp/tsconfig": "*",
    "@types/node": "^18.11.9",
    "eslint": "^8.30.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=9.0.0 <10.0.0"
  },
  "packageManager": "yarn@1.22.19"
}

{
  "extends": "@dnp/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "./dist",
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*", "test/**/*"]
}
